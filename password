import random
print('Hello, Im a password bot')
while True:
    lisst =  "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
    long_par = int(input('How long it will be:' ))
    pasword = ''
    for i in range(long_par):
        pasword += random.choice(lisst)
    print(pasword)
    go = input('Want to brake?: ')
    if go == 'yes' or go == 'Yes':
        print('continue...')
        break
    if go == 'no' or go == 'No':
        print('lets go!')
